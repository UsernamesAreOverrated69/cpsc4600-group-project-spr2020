from itertools import combinations_with_replacement
import numpy as np
count = 0 
test = np.array([[0, 0, 0],
                [0, 0, 0],
                [0, 1, 1]])
x = np.empty((3,3), dtype=int)
for comb in combinations_with_replacement(range(27),9):
    x.flat[:] = comb
    count = count + 1
    if (np.array_equal(x, test)):
        print x
        print test
        print count
        break
print 'done'
